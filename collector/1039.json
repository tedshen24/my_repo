views": [{"id": 2382445, "creationDate": 1488900620000, "isRotten": false, "quote": "Intriguing, unnerving and haunting.", "links": {"review": "http://dwightbrownink.com/nightcrawler/"}, "freshness": "fresh", "isTop": true, "url": "http://dwightbrownink.com/nightcrawler/", "isFresh": true, "critic": {"id": 18916, "name": "Dwight Brown", "thumbnailImage": "https://resizing.flixster.com/u4vJ_qeeqLzoBQk5J3K6LcLcjZI=/72x72/v1.YzszODUxO2o7MTg3OTE7MjA0ODszMDA7MzAw", "url": "/critic/dwight-brown-18916"}, "publication": {"name": "National Newspaper Publishers Association ", "url": ""}}, {"id": 2353053, "creationDate": 1474921835000, "isRotten": false, "quote": "Nightcrawler, on its surface, is a relatively straightforward thriller, built around a slightly improbable but very effective main character, a man who starts as an antihero and plunges straight ahead toward villainy.", "links": {"review": "http://www.vox.com/2014/12/5/7340681/nightcrawler-jake-gyllenhaal"}, "freshness": "fresh", "isTop": true, "url": "http://www.vox.com/2014/12/5/7340681/nightcrawler-jake-gyllenhaal", "isFresh": true, "critic": {"id": 14823, "name": "Emily VanDerWerff", "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg", "url": "/critic/emily-vanderwerff-14823"}, "publication": {"name": "Vox", "url": ""}}, {"id": 2763051, "creationDate": 1612271561000, "isRotten": false, "original_score": "4/5", "quote": "Paints an ugly picture of the news business, but it's not simply a satire or condemnation. It's frequently funny and there are some very tense action scenes, but at the end it is Gyllenhaal's bold performance that stands out.", "links": {"review": "http://www.richardcrouse.ca/nightcrawler-4-stars-it-is-gyllenhaals-bold-performance-that-stands-out/"}, "freshness": "fresh", "isTop": false, "url": "http://www.richardcrouse.ca/nightcrawler-4-stars-it-is-gyllenhaals-bold-performance-that-stands-out/", "isFresh": true, "critic": {"id": 5921, "name": "Richard Crouse", "thumbnailImage": "https://resizing.flixster.com/ccJ27MfJF5DyTthYpeMJqr02opo=/72x72/v1.YzszODgxO2o7MTg3NzY7MjA0ODszMDA7MzAw", "url": "/critic/richard-crouse"}, "publication": {"name": "Richard Crouse", "url": ""}}, {"id": 2748076, "creationDate": 1607055862000, "isRotten": false, "original_score": "9/10", "quote": "Thanks to Gyllenhaal's quirky and unhinged performance, this is a volatile ride worth taking.", "links": {"review": "http://gonewiththetwins.com/new/nightcrawler-2014/"}, "freshness": "fresh", "isTop": false, "url": "http://gonewiththetwins.com/new/nightcrawler-2014/", "isFresh": true, "critic": {"id": 27168, "name": "Mike Massie", "thumbnailImage": "https://resizing.flixster.com/cKCY6tR9EskewGPlb8BlnvWesZU=/72x72/v1.YzszNjc3O2o7MTg3OTc7MjA0ODszMDA7MzAw", "url": "/critic/mike-massie"}, "publication": {"name": "Gone With The Twins", "url": ""}}, {"id": 2737969, "creationDate": 1603842043000, "isRotten": false, "quote": "A pulse-pounding thriller set on the streets of Los Angeles, Nightcrawler stars a phenomenal Jake Gyllenhaal as a man who rises to fame by recording violent events in the middle of the night.", "links": {"review": "https://www.filmcompanion.in/streaming/netflix/best-movies-on-netflix-baahubali-the-irishman-lagaan-andhadhun/", "publication": "http://https://www.filmcompanion.in/"}, "freshness": "fresh", "isTop": false, "url": "https://www.filmcompanion.in/streaming/netflix/best-movies-on-netflix-baahubali-the-irishman-lagaan-andhadhun/", "isFresh": true, "critic": {"id": 27658, "name": "Film Companion Staff", "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg", "url": "/critic/film-companion-staff"}, "publication": {"name": "Film Companion", "url": "http://https://www.filmcompanion.in/"}}, {"id": 2716554, "creationDate": 1598417717000, "isRotten": false, "original_score": "5/5", "quote": "Dan Gilroy's pitch black satire of the 24/7 news cycle and our morbid fascination with the misery