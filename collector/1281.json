h": true, "critic": {"id": 15884, "name": "Victoria Mather", "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg", "url": "/critic/victoria-mather"}, "publication": {"name": "Daily Telegraph (UK)", "url": ""}}, {"id": 2179372, "creationDate": 1387215150000, "isRotten": false, "original_score": "4/5", "quote": "A funny, bittersweet film that opens as a cautionary tale about growing up too fast, but deepens into a movie about the unplumbable gulf between childhood and adulthood.", "links": {"review": "http://thedissolve.com/reviews/434-big/"}, "freshness": "fresh", "isTop": true, "url": "http://thedissolve.com/reviews/434-big/", "isFresh": true, "critic": {"id": 5801, "name": "Keith Phipps", "thumbnailImage": "https://resizing.flixster.com/hQXy-lCZJg_PCqIOjuDAK0jDHFg=/72x72/v1.YzsyNTk2O2c7MTg3OTY7MjA0ODsxNTA7MTUw", "url": "/critic/keith-phipps"}, "publication": {"name": "The Dissolve", "url": ""}}, {"id": 2156794, "creationDate": 1375486185000, "isRotten": false, "quote": "A delightful comedy-fantasy.", "links": {"review": "http://www.time.com/time/magazine/article/0,9171,967623,00.html"}, "freshness": "fresh", "isTop": true, "url": "http://www.time.com/time/magazine/article/0,9171,967623,00.html", "isFresh": true, "critic": {"id": 15432, "name": "Gerald Clarke", "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg", "url": "/critic/gerald-clarke"}, "publication": {"name": "TIME Magazine", "url": ""}}, {"id": 2156792, "creationDate": 1375485618000, "isRotten": false, "original_score": "3/4", "quote": "Penny Marshall brings a logic to the premise that is sustained through most of the movie. And where the other movies snickered at the sexual possibilities in the idea, she faces up to them with both candor and taste.", "links": {"review": "http://articles.philly.com/1988-06-03/entertainment/26264550_1_anne-spielberg-big-idea-movies"}, "freshness": "fresh", "isTop": true, "url": "ht