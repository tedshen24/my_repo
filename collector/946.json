"reviews": [{"isRotten": false, "url": "https://www.bostonglobe.com/arts/music/2019/04/17/beyonce-homecoming-netflix-documentary-black-history/3c58KdpBpsn5BnLVJdb7rI/story.html", "publication": {"name": "Boston Globe"}, "isTop": true, "creationDate": 1576102040000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/jenee-osterheldt", "name": "Jenee Osterheldt", "id": 18492, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "Beyonc\u00e9 crafted the masterpiece with black pride and the spirit of historically black colleges and universities.", "id": 2652968, "links": {"review": "https://www.bostonglobe.com/arts/music/2019/04/17/beyonce-homecoming-netflix-documentary-black-history/3c58KdpBpsn5BnLVJdb7rI/story.html"}}, {"isRotten": false, "url": "https://tv.avclub.com/homecoming-is-an-intimate-reminder-that-big-wins-are-ha-1834116576", "creationDate": 1576099791000, "isFresh": true, "id": 2652951, "quote": "Homecoming is not your standard concert film by any stretch.", "links": {"review": "https://tv.avclub.com/homecoming-is-an-intimate-reminder-that-big-wins-are-ha-1834116576"}, "publication": {"url": "", "name": "AV Club"}, "isTop": true, "freshness": "fresh", "critic": {"url": "/critic/shannon-miller", "name": "Shannon Miller", "id": 22722, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "original_score": "A-"}, {"isRotten": false, "url": "https://www.theguardian.com/tv-and-radio/2019/may/03/from-beyonce-to-veep-and-fleabag-what-to-stream-in-australia-in-may", "publication": {"url": "", "name": "Guardian"}, "isTop": true, "creationDate": 1563917162000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/lauren-carroll-harris", "name": "Lauren Carroll Harris", "id": 14787, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "At the glittering centre of it all is Beyonc\u00e9 herself: master maker of her own modern myth; an uber-perfectionist capitalist surveying her multimedia empire.", "id": 2607995, "links": {"review": "https://www.theguardian.com/tv-and-radio/2019/may/03/from-beyonce-to-veep-and-fleabag-what-to-stream-in-australia-in-may"}}, {"isRotten": false, "url": "http://www.pajiba.com/film_reviews/review-beyonces-homecoming-is-peak-selfmythologization.php", "publication": {"url": "", "name": "Pajiba"}, "isTop": true, "creationDate": 1556723532000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/roxana-hadadi", "name": "Roxana Hadadi", "id": 18557, "thumbnailImage": "https://resizing.flixster.com/ZhKIqqjf5fbheIszAPd9vWfntaI=/72x72/v1.YzsyNzA2O2c7MTg3NzE7MjA0ODsxNTA7MTUw"}, "quote": "Beyonc\u00e9's Coachella concert film Homecoming is peak self-mythologization, an astonishing accomplishment that centers the personal as political.", "id": 2582212, "links": {"review": "http://www.pajiba.com/film_reviews/review-beyonces-homecoming-is-peak-selfmythologization.php"}}, {"isRotten": false, "url": "https://www.theguardian.com/film/2019/apr/29/beyonce-homecoming-documentary-netflix", "publication": {"url": "", "name": "Guardian"}, "isTop": true, "creationDate": 1556552352000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/guy-lodge", "name": "Guy Lodge", "id": 14260, "thumbnailImage": "https://resizing.flixster.com/L_y19UGuH1RDcSlyC1yEa_sFr_Y=/72x72/v1.YzszOTIwO3A7MTg3NzA7MjA0ODszNDM7MzQz"}, "quote": "Non-Beylievers should check out Homecoming, too: it's unabashedly indulgent at nearly two-and-a-half hours, but conveys a more tactile, communal sense of her star magnetism than you might glean from her mirror-sheen recordings.", "id": 2581552, "links": {"review": "https://www.theguardian.com/film/2019/apr/29/beyonce-homecoming-documentary-netflix"}}, {"isRotten": false, "url": "https://www.theage.com.au/entertainment/tv-and-radio/beyonc-s-netflix-concert-film-homecoming-is-20-million-well-spent-20190423-p51gl5.html", "publication": {"url": "", "name": "The Age (Australia)"}, "isTop": true, "creationDate": 1556153456000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/karl-quinn", "name": "Karl Quinn", "id": 19925, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "Call it contradictory or call it complexity, but it's all part of what makes Beyonc\u00e9 so fascinating.", "id": 2580293, "links": {"review": "https://www.theage.com.au/entertainment/tv-and-radio/beyonc-s-netflix-concert-film-homecoming-is-20-million-well-spent-20190423-p51gl5.html"}}, {"isRotten": false, "url": "https://www.heraldsun.com.au/entertainment/movies/leigh-paatsch/your-night-in-every-movie-tonight-on-melbourne-tv-rated-or-slated/news-story/49320fd4cb370b811443fa0d300a7cf9", "creationDate": 1587491886000, "isFresh": true, "id": 2684788, "quote": "The behind-the-scenes stuff is indeed fascinating, but no match for the revelatory experience of Beyonce's performance itself, which lives up to everything you may have heard.", "links": {"review": "https://www.heraldsun.com.au/entertainment/movies/leigh-paatsch/your-night-in-every-movie-tonight-on-melbourne-tv-rated-or-slated/news-story/49320fd4cb370b811443fa0d300a7cf9"}, "publication": {"url": "", "name": "Herald Sun (Australia)"}, "isTop": false, "freshness": "fresh", "critic": {"url": "/critic/leigh-paatsch", "name": "Leigh Paatsch", "id": 13081, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "original_score": "4/5"}, {"isRotten": false, "url": "https://hyperallergic.com/498113/beyonce-homecoming-netflix-coachella/", "publication": {"url": "", "name": "Hyperallergic"}, "isTop": false, "creationDate": 1582663942000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/zoe-guy", "name": "Zoe Guy", "id": 26702, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "Too many people still treat African American culture with willful amnesia or purposeful neglect. This kind of affirmation and condensed schooling is just what we need to cut through the nonsense.", "id": 2672346, "links": {"review": "https://hyperallergic.com/498113/beyonce-homecoming-netflix-coachella/"}}, {"isRotten": false, "url": "https://www.culturewhisper.com/r/netflix/homecoming_beyonce_documentary_netflix/13638", "publication": {"url": "", "name": "Culture Whisper"}, "isTop": false, "creationDate": 1576102580000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/ella-kemp", "name": "Ella Kemp", "id": 20142, "thumbnailImage": "https://resizing.flixster.com/tuFn3B133gx9H6fxkkuNwmmxjLU=/72x72/v1.YzsyOTc0O2o7MTg3Njk7MjA0ODs0MDA7NDAw"}, "quote": "This show represented something different, as it was more in tune with her roots than what a mostly white audience at the music festival in California could begin to understand.", "id": 2652973, "links": {"review": "https://www.culturewhisper.com/r/netflix/homecoming_beyonce_documentary_netflix/13638"}}, {"isRotten": false, "url": "https://www.mirror.co.uk/film/beyonce-homecoming-beyonce-changes-pink-14722812", "publication": {"url": "", "name": "Daily Mirror (UK)"}, "isTop": false, "creationDate": 1576102452000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/jo-anne-rowney", "name": "Jo-Anne Rowney", "id": 18367, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "She of course looks amazing.", "id": 2652969, "links": {"review": "https://www.mirror.co.uk/film/beyonce-homecoming-beyonce-changes-pink-14722812"}}, {"isRotten": false, "url": "https://www.express.co.uk/entertainment/films/1117897/Beyonce-Homecoming-documentary-Netflix-yellow-pink-jumper-Balmain-BAC-costume", "publication": {"url": "", "name": "Daily Express (UK)"}, "isTop": false, "creationDate": 1576101702000, "isFresh": true, "freshness": "fresh",