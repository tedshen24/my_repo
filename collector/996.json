vudu": {"url": "https://www.vudu.com/content/movies/details/La-Bamba/13777", "type": "buy"}, "flixster": {"url": "la-bamba", "movieId": "ID: 2000017472; SKU:457647; Type: B (SD Buy)", "type": "buy"}, "iTunes": {"url": "https://itunes.apple.com/movie/la-bamba/id270007203?uo=5", "price": "12.99"}}, "reviews": {"total": 43, "links": {"alternate": "//www.rottentomatoes.com/m/la_bamba/#reviews", "self": "api.flixster.com/movies/la-bamba/reviews.json?review_type=mob&country=us&page=1", "prev": "api.flixster.com/movies/la-bamba/reviews.json?review_type=mob&country=us&page=0", "next": "api.flixster.com/movies/la-bamba/reviews.json?review_type=mob&country=us&page=2"}, "reviews": [{"isRotten": true, "url": "https://www.latimes.com/archives/la-xpm-1987-07-24-ca-3804-story.html", "publication": {"url": "", "name": "Los Angeles Times"}, "isTop": true, "creationDate": 1585086773000, "isFresh": false, "freshness": "rotten", "critic": {"url": "/critic/sheila-benson", "name": "Sheila Benson", "id": 545, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "You can have a perfectly pleasant time at \"La Bamba\" but you come out still hungry to understand the forces that shaped [Ritchie Valens].", "id": 2679032, "links": {"review": "https://www.latimes.com/archives/la-xpm-1987-07-24-ca-3804-story.html"}}, {"isRotten": false, "url": "https://www.nytimes.com/1987/07/24/movies/film-la-bamba-a-musical-biography.html", "publication": {"url": "", "name": "New York Times"}, "isTop": true, "creationDate": 1585086614000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/janet-maslin", "name": "Janet Maslin", "id": 341, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "As Valens, Lou Diamond Phillips has a sweetness and sincerity that in no way diminish the toughness of his onstage persona. The role is blandly written, but Mr. Phillips gives Valens backbone.", "id": 2679031, "links": {"review": "https://www.nytimes.com/1987/07/24/movies/film-la-bamba-a-musical-biography.html"}}, {"isRotten": false, "url": "https://www.chicagotribune.com/news/ct-xpm-1987-07-24-8702240094-story.html", "creationDate": 1585086492000, "isFresh": true, "id": 2679030, "quote": "The movie works surprisingly for reasons that have nothing to do with Valens` small list of hits. La Bamba is really just a very good family story about a pushy mother proud of one son, derisive of another.", "links": {"review": "https://www.chicagotribune.com/news/ct-xpm-1987-07-24-8702240094-story.html"}, "publication": {"url": "", "name": "Chicago Tribune"}, "isTop": true, "freshness": "fresh", "critic": {"url": "/critic/gene-siskel", "name": "Gene Siskel", "id": 308, "thumbnailImage": "https://resizing.flixster.com/-pwFyOJ0ZEfxNsvV19EmCNsmOqk=/72x72/v1.YzszNTIxO2o7MTg3NzY7MjA0ODszMDA7MzAw"}, "original_score": "3/4"}, {"isRotten": true, "url": "http://content.time.com/time/subscriber/article/0,33009,965242,00.html", "publication": {"url": "", "name": "TIME Magazine"}, "isTop": true, "creationDate": 1585086326000, "isFresh": false, "freshness": "rotten", "critic": {"url": "/critic/richard-corliss", "name": "Richard Corliss", "id": 1227, "thumbnailImage": "https://resizing.flixster.com/tvlKOiED6ZR55pcIRwNevFIDvfs=/72x72/v1.YzsxNjUyO2c7MTg3OTc7MjA0ODszODszOQ"}, "quote": "How many films can be squeezed out of this formula?", "id": 2679028, "links": {"review": "http://content.time.com/time/subscriber/article/0,33009,965242,00.html"}}, {"isRotten": false, "url": "https://www.chicagotribune.com/news/ct-xpm-1987-07-24-8702240086-story.html", "creationDate": 1585084990000, "isFresh": true, "id": 2679015, "quote": "Melodrama is inherently a broad genre -- one that projects inner psychological conflicts onto towering outside forces -- and in the overall thrust of his film, Valdez excels in conveying a world dominated by a gigantic, impersonal power.", "links": {"review": "https://www.chicagotribune.com/news/ct-xpm-1987-07-24-8702240086-story.html"}, "publication": {"url": "", "name": "Chicago Tribune"}, "isTop": true, "freshness": "fresh", "critic": {"url": "/critic/dave-kehr", "name": "Dave Kehr", "id": 15, "thumbnailImage": "https://resizing.flixster.com/9JKqg6sYG8Ma4MxlrqIMwc3jppc=/72x72/v1.YzszNjQ3O2o7MTg3Njc7MjA0ODszMDA7MzAw"}, "original_score": "3/4"}, {"isRotten": false, "url": "https://www.orlandosentinel.com/news/os-xpm-1987-07-24-0180340162-story.html", "publication": {"name": "Orlando Sentinel"}, "isTop": true, "creationDate": 1585084582000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/jay-boyar", "name": "Jay Boyar", "id": 3209, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "La Bamba has a lot going for it besides its compelling teen myth. As might have been predicted, the music is terrific.", "id": 2679011, "links": {"review": "https://www.orlandosentinel.com/news/os-xpm-1987-07-24-0180340162-story.html"}}, {"isRotten": false, "url": "http://www.thefilmyap.com/movies/la-bamba-1987/", "creationDate": 1589807132000, "isFresh": true, "id": 2690693, "quote": "An energetic picture filled with light and music, and Lou Diamond Phillips is an easygoing charismatic presence as Ritchie Valens, so sweet-hearted he's almost angelic. ", "links": {"review": "http://www.thefilmyap.com/movies/la-bamba-1987/"}, "publication": {"url": "", "name": "The Film Yap"}, "isTop": false, "freshness": "fresh", "critic": {"url": "/critic/christopher-lloyd", "name": "Christopher Lloyd", "id": 13878, "thumbnailImage": "https://resizing.flixster.com/cYoGrLCS2Y5_L9cU5pSCAzzpgqk=/72x72/v1.YzszMTA3O2o7MTg3Njc7MjA0ODszMDA7MzAw"}, "original_score": "4/5"}, {"isRotten": false, "url": "https://www.sun-sentinel.com/news/fl-xpm-1987-07-24-8703010949-story.html", "creationDate": 1585086936000, "isFresh": true, "id": 2679033, "quote": "Valdez's simple-yet-elegant visual style is on target and his script uses the elements of Valens' life to the fullest, almost creating an underlying poetic rhythm to the film.", "links": {"review": "https://www.sun-sentinel.com/news/fl-xpm-1987-07-24-8703010949-story.html"}, "publication": {"name": "South Florida Sun-Sentinel"}, "isTop": false, "freshness": "fresh", "critic": {"url": "/critic/juan-carlos-coto", "name": "Juan Carlos Coto", "id": 15588, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "original_score": "4/4"}, {"isRotten": false, "url": "https://www.csmonitor.com/1987/0724/lfl24.html", "publication": {"url": "", "name": "Christian Science Monitor"}, "isTop": false, "creationDate": 1585086227000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/david-sterritt", "name": "David Sterritt", "id": 149, "thumbnailImage": "https://resizing.flixster.com/V6dEv--GKwQKKq8IENXNq4Ori4I=/72x72/v1.YzsyMDI3O2o7MTg3Njc7MjA0ODs3Mjs3Mg"}, "quote": "It's easy to forgive the thudding omens and ironies, since they appear in such a life-affirming context. The purpose of La Bamba is not to mourn Valens's death but to celebrate his life, with special attention to the great joy he found in music.", "id": 2679027, "links": {"review": "https://www.csmonitor.com/1987/0724/lfl24.html"}}, {"isRotten": false, "url": "https://archive.macleans.ca/article/1987/7/27/la-bamba", "publication": {"url": "", "name": "Maclean's Magazine"}, "isTop": false, "creationDate": 1585086128000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/nicholas-jennings", "name": "Nicholas Jennings", "id": 26821, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "quote": "With a convincing performance by newcomer Phillips -- and the exhilarating Latin rockers Los Lobos, who bring Valens's sound back to life -- La Bamba is a refreshing blast from the past.", "id": 2679025, "links": {"review": "https://archive.macleans.ca/article/1987/7/27/la-bamba"}}, {"isRotten": false, "url": "https://cdnc.ucr.edu/?a=d&d=DS19870727.2.188&e=------198-en--20--1--txt-txIN--------1", "publication": {"url": "", "name": "Gannett News Service"}, "isTop": false, "creationDate": 1585084775000, "isFresh": true, "freshness": "fresh", "critic": {"url": "/critic/jack-garner", "name": "Jack Garner", "id": 1, "thumbnailImage": "https://resizing.flixster.com/dx-nUBKZbAhzHYcTIASKPGU2NR8=/72x72/v1.YzsxNTc4O2c7MTg3Njg7MjA0ODszODs0MQ"}, "quote": "Valens, meanwhile, is played with likeable charm and refreshing innocence by relative newcomer Lou Diamond Phillips. Among the large supporting cast, Rosana De Soto stands out as Ritchie's proud and loving mother.", "id": 2679014, "links": {"review": "https://cdnc.ucr.edu/?a=d&d=DS19870727.2.188&e=------198-en--20--1--txt-txIN--------1"}}, {"isRotten": false, "url": "https://www.radiotimes.com/film/9c6w/la-bamba/", "creationDate": 1585084275000, "isFresh": true, "id": 2679007, "quote": "Most pop biopics are tepid affairs, but the same cannot be said for director Luis Valdez's engrossing re-creation of Ritchie Valens's short life.", "links": {"review": "https://www.radiotimes.com/film/9c6w/la-bamba/"}, "publication": {"url": "", "name": "Radio Times"}, "isTop": false, "freshness": "fresh", "critic": {"url": "/critic/john-marriott", "name": "John Marriott", "id": 14472, "thumbnailImage": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/user.none.tmb.jpg"}, "original_score": "3/5"}]}, "casts": {"screenwriters": [{"url": "/celebrity/luis_valdez/", "name": "Luis Valdez", "thumbnailImg": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/actor.default.tmb.gif"}], "directors": [{"url": "/celebrity/luis_valdez/", "name": "Luis Valdez", "thumbnailImg": "https://d2a5cgar23scu2.cloudfront.net/static/images/redesign/actor.default.tmb.gif"}], "creators": [], "producers": [{"url": "/celebrity/bill_borden/", "name": "Bill Borden", "thumbnailImg": "https://d2a5cgar23scu2.clou